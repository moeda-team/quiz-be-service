{
  "version": 2,
  "functions": {
    "api/index.ts": {
      "maxDuration": 10
    }
  },
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["prisma/schema.prisma", "prisma/migrations/**"]
      }
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "api/index.ts" }],
  "buildCommand": "npm run vercel-build"
}
